    "concurrently": "^8.2.2",
    "eslint": "^8.0.0",
    "eslint-config-next": "15.0.3",
    "jest": "^29.7.0",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
